generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model AstronomyPictureOfDay {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  date String?
  explanation String?
  hdurl String?
  media_type String?
  service_version String?
  title String?
  url String?
}

model Neo {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  mass String?
  height String?
  width String?
  radius String?
}
