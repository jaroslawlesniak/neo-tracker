generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

enum ReactionType {
  heart
}

model AstronomyPictureOfDay {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  date String?
  explanation String?
  hdurl String?
  media_type String?
  service_version String?
  title String?
  url String?
  reactions Reaction[]
}

model Reaction {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  astronomy_picture_of_day AstronomyPictureOfDay? @relation(fields: [astronomy_picture_of_day_id], references: [id])
  astronomy_picture_of_day_id String? @db.ObjectId
  device_id String
  type ReactionType @default(heart)
  added_at String
}

model Neo {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  mass String?
  height String?
  width String?
  radius String?
}
